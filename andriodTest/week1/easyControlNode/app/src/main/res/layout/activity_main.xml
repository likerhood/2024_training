<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/bnt_click_to_buttonTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/mainActivityButtonTest"
        android:textColor="@color/black"
        android:backgroundTint="@color/buttonColorDefault"
        android:textSize="25sp"/>

    <Button
        android:id="@+id/bnt_to_calculator"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="跳转到计算器"
        android:textColor="@color/black"
        android:backgroundTint="@color/buttonColorDefault"
        android:textSize="25sp"/>

    <Button
        android:id="@+id/bnt_to_lifyCircle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="生命周期测试"
        android:textColor="@color/black"
        android:backgroundTint="@color/buttonColorDefault"
        android:textSize="25sp"/>

    <Button
        android:id="@+id/bnt_to_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="登录界面"
        android:textColor="@color/black"
        android:backgroundTint="@color/buttonColorDefault"
        android:textSize="25sp"/>



</LinearLayout>